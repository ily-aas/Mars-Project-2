@model webComplaintsModel

<form method="post" asp-action="SubmitForm" asp-controller="webComplaints">
    <div class="form-group">
        <label asp-for="webComplaintFname">First Name:</label>
        <input asp-for="webComplaintFname" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="webComplaintLname">Last Name:</label>
        <input asp-for="webComplaintLname" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="webComplaintEmail">Email:</label>
        <input asp-for="webComplaintEmail" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="webComplaintMnumber">Mobile Number:</label>
        <input asp-for="webComplaintMnumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="webComplaintDetails">Complaint Details:</label>
        <textarea asp-for="webComplaintDetails" class="form-control"></textarea>
    </div>
    <div class="form-group">
        <label asp-for="webComplaintIP">IP Address:</label>
        <input asp-for="webComplaintIP" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
